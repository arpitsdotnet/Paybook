@model IEnumerable<Paybook.ServiceLayer.Models.ActivityModel>

@if (Model == null || Model.Count() == 0)
{
    <div class="alert alert-info">We did not find any activity.</div>
}
else
{
    <div class="activity-timeline">
        <table class="table table-striped table-hover ">
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.Raw(item.TextHtml)
                    </td>
                </tr>
            }
        </table>
    </div>
}