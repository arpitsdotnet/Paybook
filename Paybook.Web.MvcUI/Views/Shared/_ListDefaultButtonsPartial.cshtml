@model int?

<button type="button" class="btn btn-primary btn-sm" title="Edit" onclick="window.location.href='@Url.Action("Edit", new { id = Model })';"><i class="glyphicon glyphicon-pencil"></i></button>
<button type="button" class="btn btn-danger btn-sm" title="Delete" onclick="window.location.href='@Url.Action("Delete", new { id = Model })';"><i class="glyphicon glyphicon-trash"></i></button>

